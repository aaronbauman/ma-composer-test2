    "libraries/chosen": {
        "type": "package",
        "package": {
            "name": "library/chosen",
            "version": "1.0",
            "type": "drupal-library",
            "dist": {
                "url": "https://github.com/harvesthq/chosen/releases/download/v1.6.1/chosen_v1.6.1.zip",
                "type": "zip"
            }
        }
