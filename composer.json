            "name": "drupal/ma_datetime_range_helper",
            "type": "sprout-module",
            "version": "1.0",
            "source": {
                "url": "git@bitbucket.org:messageagency/ma_datetime_range_helper.git",
                "type": "git",
                "reference": "master"
            }
        }
    },
    "ma_gridmenu": {
        "type": "package",
        "package": {
            "name": "drupal/ma_gridmenu",
            "type": "sprout-module",
            "version": "1.0",
            "source": {
                "url": "git@bitbucket.org:messageagency/ma_gridmenu.git",
                "type": "git",
                "reference": "master"
            }
        }
    },
    "ma_filters": {
        "type": "package",
        "package": {
            "name": "drupal/ma_filters",
            "type": "sprout-module",
            "version": "1.0",
            "source": {
                "url": "git@bitbucket.org:messageagency/ma_filters.git",
                "type": "git",
                "reference": "master"
            }
        }
    },
    "ma_signpost_icons": {
        "type": "package",
        "package": {
            "name": "drupal/ma_signpost_icons",
            "type": "sprout-module",
            "version": "1.0",
            "source": {
                "url": "git@bitbucket.org:messageagency/ma_signpost_icons.git",
                "type": "git",
                "reference": "master"
            }
        }
    },
    "ma_twig": {
        "type": "package",
        "package": {
            "name": "drupal/ma_twig",
            "type": "sprout-module",
            "version": "1.0",
            "source": {
                "url": "git@bitbucket.org:messageagency/ma_twig.git",
                "type": "git",
                "reference": "master"
            }
        }
    },
    "ma_parent": {
        "type": "package",
        "package": {
            "name": "drupal/ma_parent",
            "type": "drupal-theme",
            "version": "1.0",
            "source": {
                "url": "git@bitbucket.org:messageagency/ma_parent.git",
