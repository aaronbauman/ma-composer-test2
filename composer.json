{
  "name": "messageagency/sprout",
  "description": "Message Agency Sprout",
  "type": "project",
  "license": "MIT",
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "block_access": {
